{
  "type": "extension",
  "name": "tencent_asr_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.6"
    }
  ],
  "api": {
    "property": {
      "app_id": {
        "type": "string"
      },
      "secret_id": {
        "type": "string"
      },
      "secret_key": {
        "type": "string"
      },
      "engine_model_type":{
        "type": "string"
     }
    },
    "audio_frame_in": [
      {
        "name": "pcm_frame",
        "property": {}
      }
    ],
    "cmd_in": [
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "time": {
            "type": "int64"
          },
          "duration_ms": {
            "type": "int64"
          },
          "language": {
            "type": "string"
          },
          "text": {
            "type": "string"
          },
          "is_final": {
            "type": "bool"
          },
          "stream_id": {
            "type": "uint32"
          },
          "end_of_segment": {
            "type": "bool"
          }
        }
      }
    ]
  }
}