{
  "type": "extension",
  "name": "tts_test_tool_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.6"
    }
  ],
  "api": {
    "property": {
      "duration": {
        "type": "string"
      },
      "loop_count": {
        "type": "string"
      },
